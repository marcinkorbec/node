<section class="summary">
    <h3>Wybrane ciastko:</h3>
    <ul>
        <li>{{cookie.base}} - {{find-price bases cookie.base}}zł</li>
    </ul>

    <h3>Wybrane dodatki:</h3>
    <ul>
        {{#each cookie.addons}}
            <li>{{this}} {{find-price @root.addons this}}zł</li>
        {{/each}}
    </ul>
</section>