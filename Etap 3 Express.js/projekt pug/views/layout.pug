doctype html
mixin item(title, url)
	li
		a(href=url class=path == url ? 'active' : '')=title

html
	head
		title= title
		link(rel='stylesheet', href='/stylesheets/style.css')
	body
		div.container
			nav
				ul
					+item('Aktualności', '/news')
					+item('Strona główna', '/')
					+item('Quiz', '/quiz')
					+item('Logowanie', '/admin')

			block content
